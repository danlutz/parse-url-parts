{
  "name": "parse-url-parts",
  "version": "1.0.1",
  "description": "Parse parts of uniform resource locators",
  "main": "dist/parseUrlParts.js",
  "scripts": {
    "clean": "rimraf dist",
    "preversion": "jest",
    "version": "npm run build",
    "build": "npm run clean && npm run transpile",
    "transpile": "babel src --out-dir dist",
    "lint": "eslint src",
    "prettier": "prettier \"**/*.+(js|jsx|json|yml|yaml|html|css|less|scss|ts|tsx|md|mdx|graphql|vue|java|php)\"",
    "format": "npm run prettier -- --write",
    "validate": "npm run lint && npm run prettier -- --list-different",
    "jest": "jest",
    "test": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danlutz/parse-url-parts.git"
  },
  "keywords": [
    "url",
    "url-parser",
    "url-parts",
    "url parser",
    "url parts",
    "ftp url",
    "localhost url"
  ],
  "author": "Daniel Lutz <daniel-lutz@live.at>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danlutz/parse-url-parts/issues"
  },
  "homepage": "https://github.com/danlutz/parse-url-parts#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "jest-config": "^24.7.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3"
  }
}
